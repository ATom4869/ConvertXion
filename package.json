{
  "name": "file-converter",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "nextjs",
    "backend"
  ],
  "scripts": {
    "backend": "cd backend && yarn start",
    "frontend": "cd nextjs && yarn dev",
    "start": "concurrently \"yarn backend\" \"yarn frontend\"",
    "engine": "cd rust-backend && cargo run RUST_BACKTRACE=1",
    "cleanup": "cd rust-backend && cargo clean",
    "rbuild": "cd rust-backend && cargo build --release -j 4",
    "radd": "cd rust-backend && cargo add",
    "dev": "cd nextjs && yarn dev --clear",
    "build": "cd nextjs && next build",
    "node-build": "cd backend && yarn build",
    "lint": "cd nextjs && next lint"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
